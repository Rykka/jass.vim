if version < 600
 syntax clear
endif

let s:luapath= fnameescape(globpath(&rtp,"syntax/lua.vim"))
if filereadable(s:luapath)
    unlet! b:current_syntax
    exe "syn include @jassLuaScript ".s:luapath
    syn region jassLuaRegion matchgroup=jassMacro fold start="//! externalblock extension=lua .*" end="//! endexternalblock" contains=@jassLuaScript
    syn cluster jassCmntList add=jassLuaRegion
endif
unlet s:luapath
let b:current_syntax = "jass"

syn match decNumber "\.\d*"
syn match decNumber "\<\%(0[dD]\)\=\%(0\|[1-9]\d*\%(_\d\+\)*\)\>"
syn match decNumber "\<\%(0\|[1-9]\d*\%(_\d\+\)*\)\.\d\+\%(_\d\+\)*\>"
syn match decNumber "[]\=\<\d[[:digit:]_]*\.[[:digit:]_]*\([eE][\-+]\=\d\+\)\="
syn match octNumber "0[0-7][0-7]\+"
syn match hexNumber "0[xX][0-9a-fA-F]\+"
syn match hexNumber "$[0-9a-fA-F]\+"
syn cluster jassNumbers contains=decNumber,octNumber,hexNumber

syn region jassString start=+L\="+ skip=+\\\\\|\\"+ end=+"+ contains=cSpecial,@Spell

syn match jassComment "//.*" contains=@Spell
syn region vjassComment fold start="/\*" end="\*/" contains=vjassComment
syn match jassSpecialComment "[A-Z0-9]\+:" containedin=vjassComment,jassComment
syn match jassTODO    "\(NOTE\|TODO\|XXX\|FIXME\):\=" containedin=jassComment,vjassComment
syn cluster jassCmntList contains=jassComment,vjassComment

syn match jassMacro "//! runtextmacro"
syn region jassMacroR matchgroup=jassMacro fold start="//! textmacro\>" end="//! endtextmacro" contains=@jassCtrlList contained
syn cluster jassCmntList add=jassMacroR,jassMacro



syn keyword jassPreprocessorKeyword debug
syn match jassFuncInterface "\<function interface\>"

syn keyword jassOperator and or not
syn match jassOperator "\(==\|!=\|>=\|<=\|=\~\|!\~\|>\|<\|=\)"
syn match jassOperator "[\.\-+*\\]" skipwhite

syn keyword jassBoolean true false null
syn match jassReplace "\$[^$]*\$"
syn match jassRawCode "'[^'][^'][^'][^']'"
syn match jassChar "'[^']'"

syn match jassDelimiter "[/\\]$"
" callback function 
syn match jassDelimiter "\((.\{-}\)\@<=\<function\>\(.\{-})\)\@="

syn keyword jassThis this
syn keyword jassType thistype key
syn keyword jassLabel takes returns defaults
syn keyword jassLabel extends delegate readonly operator implement optional initializer requires requires needs uses stub threaded
syn keyword jassStorage local private public native constant type static

syn cluster jassExprList contains=jassBoolean,jassOperator,jassReplace,jassRawCode,jassChar,jassDelimiter,jassType,jassStorage,jassString,jassNumbers,jassConst,jassBJConst

syn keyword jassKeyword set call return contained
syn cluster jassStmtList contains=@jassExprList,jassThis,jassLabel,jassKeyword,jassPreprocessorKeyword,jassFuncInterface,jassFuncname,jassBJFuncname

syn keyword jassRepeat exitwhen
syn keyword jassCond elseif else then
syn region jassLoopR matchgroup=jassRepeat fold start="\<loop\>" end="\<endloop\>" contains=@jassCtrlList,@jassCmntList contained
syn region jassCondR matchgroup=jassCond fold start="\<if\>" end="\<endif\>" contains=@jassCtrlList,@jassCmntList contained

syn cluster jassCtrlList contains=@jassStmtList,jassRepeat,jassCond,jassLoopR,jassCondR


syn region jassFunctR matchgroup=jassStructure fold start="\<function\>\( interface\)\@!\ze.*returns" end="\<endfunction\>" contains=@jassCtrlList,@jassCmntList
syn region jassMethdR matchgroup=jassStructure fold start="\<method\>" end="\<endmethod\>" contains=@jassCtrlList,@jassCmntList containedin=jassStructR
syn region jassInterR matchgroup=jassStructure fold start="\<interface\>" end="\<endinterface\>" contains=@jassCtrlList,@jassCmntList

syn region jassGlobalR matchgroup=jassStructure fold start="\<globals\>" end="\<endglobals\>" contains=@jassExprList,@jassCmntList

syn region jassModulR matchgroup=jassStructure fold start="\<module\>" end="\<endmodule\>" contains=jassMethdR,jassStorage,@jassExprList,jassLabel,@jassCmntList
syn region jassStructR matchgroup=jassStructure fold start="\<struct\>" end="\<endstruct\>" contains=jassModulR,jassStorage,@jassExprList,jassLabel,@jassCmntList

syn region jassScopeR matchgroup=jassStructure fold start="\<scope\>" end="\<endscope\>" contains=jassStructR,jassGlobalR,jassFunctR,jassStorage,jassLabel,@jassCmntList,jassModulR
syn region jassLibR matchgroup=jassStructure fold start="\<library\%[_once]\>" end="\<endlibrary\>" contains=jassStructR,jassFunctR,jassGlobalR,jassStorage,jassLabel,@jassCmntList,jassModulR

" Zinc
syn keyword zjassStruct library module interface function struct method globals contained
syn keyword zjassStruct type extends optional operator contained
syn keyword zjassCond if else for while contained
syn region jassZincR matchgroup=jassMacro fold start="//! zinc" end="//! endzinc" contains=jassComment,vjassComment,zjassBlock,zjassStruct,jassLabel,jassStorage,zJassCond,@jassStmtList
syn region zjassBlock start=/{/ end=/}/  transparent fold keepend extend



""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""
" Define the default highlighting.
" For version 5.7 and earlier: only when not done already
" For version 5.8 and later: only when an item doesn't have highlighting yet
if version >= 508 || !exists("did_jass_syntax_inits")
    if version < 508
    let did_jass_syntax_inits = 1
    command -nargs=+ HiLink hi link <args>
    else
    command -nargs=+ HiLink hi def link <args>
    endif

    HiLink jassConst Constant
    HiLink jassBJConst Special
    HiLink jassFuncname Function
    HiLink jassBJFuncname Delimiter
    HiLink jassComment Comment
    HiLink vjassComment Comment
    HiLink jassSpecialComment SpecialComment
    HiLink jassFuncInterface Preproc
    HiLink jassPreprocessorKeyword Preproc

    HiLink jassCond Statement
    HiLink zjassCond Statement
    HiLink jassStructure Structure
    HiLink zjassStruct Structure
    HiLink jassStorage StorageClass

    HiLink hexNumber Number
    HiLink decNumber Number
    HiLink octNumber Number
    HiLink binNumber Number

    HiLink jassType Type
    HiLink jassString String
    HiLink jassRawCode Character
    HiLink jassChar Character
    HiLink jassOperator Operator
    HiLink jassReplace SpecialChar
    HiLink jassDelimiter Delimiter
    HiLink jassBoolean Constant
    HiLink jassThis Constant
    HiLink jassRepeat Repeat
    HiLink jassKeyword Keyword
    HiLink jassLabel Label
    HiLink jassMacro Condition

    HiLink jassTODO Todo

    delcommand HiLink
endif

